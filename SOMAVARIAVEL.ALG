Algoritmo "SomaVariavel"


Var
     indice, soma, k : inteiro

Inicio
         indice <- 13
         soma <- 0
         k <- 0
         
         enquanto (K < indice) faça
         K <- K + 1
         Soma <- soma + K
         fimenquanto
         escreval
         
         escreval ("Valor da soma: ", soma)
         escreval
         
         

Fimalgoritmo